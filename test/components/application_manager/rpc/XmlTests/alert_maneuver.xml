    <!-- AlertManeuver: All parameters -->
    <!-- TTSChunks and SoftButtons -->
    <test testName="AlertManeuver_AllParams" pause="2000">
          <request>
                   <params>
                           <Integer function_id = "28"/>
			   <Integer correlation_id = "1" />
                   </params>
                   <message_params>
				<Vector name="ttsChunks">
					<TTSChunk>
						<String text="First Alert"/>
						<String type="TEXT"/>
					</TTSChunk>
					<TTSChunk>
						<String text="Second Alert"/>
						<String type="TEXT"/>
					</TTSChunk>
				</Vector>
				<Vector name="softButtons">
					<SoftButton>
						<String type="BOTH" />
						<String text="Close" />
						<image>
							<String value="icon.png" />
							<String imageType="DYNAMIC" />
						</image>
						<Boolean isHighlighted="true" />
						<Integer softButtonID="44" />
						<String systemAction="DEFAULT_ACTION" />
					</SoftButton>
				</Vector>
                   </message_params>
          </request>
          <response>
                   <params>
                           <Integer correlation_id = "1" />
                   </params>
                   <message_params>
                           <String resultCode = "SUCCESS" />
                   </message_params>
          </response>
          <HMIRequestFirst>
                   <params>
                           <Integer function_id = "81" />
                   </params>
                   <message_params>
                           <String method = "Navigation.AlertManeuver" />
                           <params>
				   <Vector name="softButtons">
					<SoftButton>
						<String type="BOTH" />
						<String text="Close" />
						<image>
							<Patch value="icon.png" />
							<String imageType="DYNAMIC" />
						</image>
						<Boolean isHighlighted="true" />
						<Integer softButtonID="44" />
						<String systemAction="DEFAULT_ACTION" />
					</SoftButton>
				   </Vector>
                           </params>
                   </message_params>
          </HMIRequestFirst>
          <HMIRequestSecond>
                   <params>
                           <Integer function_id = "40" />
                   </params>
                   <message_params>
                           <String method = "TTS.Speak" />
                           <params>
                                   <Integer appID = "65537" />
                                   <Vector name = "ttsChunks" >
                                           <TTSChunk>
                                                     <String text = "First Alert" />
                                                     <String type = "TEXT" />
                                           </TTSChunk>
                                           <TTSChunk>
                                                     <String text = "Second Alert" />
                                                     <String type = "TEXT" />
                                           </TTSChunk>
                                   </Vector>
                           </params>
                   </message_params>
          </HMIRequestSecond>
    </test>
